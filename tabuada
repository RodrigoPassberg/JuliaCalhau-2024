#include <iostream>

int main() {
    int numero;

    std::cout << "Digite um numero para ver sua tabuada: ";
    std::cin >> numero;

    int contador = 1;

    std::cout << "Tabuada de " << numero << ":\n";
    while (contador <= 10) {
        std::cout << numero << " x " << contador << " = " << (numero * contador) << std::endl;
        contador++;
    }

    return 0;
}
