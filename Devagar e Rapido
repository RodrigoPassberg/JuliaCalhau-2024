#include <stdio.h>
#include <locale.h>

int main(){
    
    int velocidade;
    int distancia;
    char nome_carro[252];
    
    printf("Digite o nome do carro: ");
    scanf("%s", nome_carro);
    
    printf("Digite a velocidade do carro:");
    scanf("%d", &distancia);
    
     if(velocidade<100){
       printf("Rapido");
       }else{
       printf("Devagar");
       }
       return 0;  
}
